datasource db {
    // could be postgresql or mysql
    provider = "sqlite"
    url      = "file:database.db"
}

generator db {
    provider = "go run github.com/prisma/prisma-client-go"
    output           = "./db"
    package          = "db"
}

model File {
    id String @id @unique
    mime String
    ext String
    secret String
    timestamp DateTime @default(now())
}